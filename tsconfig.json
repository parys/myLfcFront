{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "lib": [
      "es2020",
      "dom"
    ],
    "module": "ES2020",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2022",
    "strict": false,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/out-tsc",
    "paths": {
      "@environments/*": [
        "./src/environments/*"
      ],
      "@accounts/*": [
        "./src/app/accounts/*"
      ],
      "@admin/*": [
        "./src/app/admin/*"
      ],
      "@auth/*": [
        "./src/app/base/auth/*"
      ],
      "@base/*": [
        "./src/app/base/*"
      ],
      "@chat/*": [
        "./src/app/chat/*"
      ],
      "@constants/*": [
        "./src/app/constants/*"
      ],
      "@core/*": [
        "./src/app/core/*"
      ],
      "@clubs/*": [
        "./src/app/clubs/*"
      ],
      "@contracts/*": [
        "./src/app/contracts/*"
      ],
      "@comments/*": [
        "./src/app/comments/*"
      ],
      "@domain/*": [
        "./src/app/domain/*"
      ],
      "@editor/*": [
        "./src/app/editor/*"
      ],
      "@faq/*": [
        "./src/app/faq/*"
      ],
      "@faq-categories/*": [
        "./src/app/faq-categories/*"
      ],
      "@faq-items/*": [
        "./src/app/faq-items/*"
      ],
      "@forum/*": [
        "./src/app/forum/*"
      ],
      "@images/*": [
        "./src/app/images/*"
      ],
      "@injuries/*": [
        "./src/app/injuries/*"
      ],
      "@layout/*": [
        "./src/app/layout/*"
      ],
      "@lazy-modules/*": [
        "./src/app/lazy-modules/*"
      ],
      "@matches/*": [
        "./src/app/matches/*"
      ],
      "@match-events/*": [
        "./src/app/match-events/*"
      ],
      "@match-persons/*": [
        "./src/app/match-persons/*"
      ],
      "@materials/*": [
        "./src/app/materials/*"
      ],
      "@material-categories/*": [
        "./src/app/material-categories/*"
      ],
      "@notices/*": [
        "./src/app/notices/*"
      ],
      "@notifications/*": [
        "./src/app/notifications/*"
      ],
      "@network/*": [
        "./src/app/network/*"
      ],
      "@persons/*": [
        "./src/app/persons/*"
      ],
      "@pms/*": [
        "./src/app/pms/*"
      ],
      "@od/*": [
        "./src/app/od/*"
      ],
      "@polls/*": [
        "./src/app/polls/*"
      ],
      "@role-groups/*": [
        "./src/app/role-groups/*"
      ],
      "@seasons/*": [
        "./src/app/seasons/*"
      ],
      "@stadiums/*": [
        "./src/app/stadiums/*"
      ],
      "@static-pages/*": [
        "./src/app/static-pages/*"
      ],
      "@static-pages-editor/*": [
        "./src/app/static-pages-editor/*"
      ],
      "@transfers/*": [
        "./src/app/transfers/*"
      ],
      "@users/*": [
        "./src/app/users/*"
      ],
      "@wal/*": [
        "./src/app/wal/*"
      ],
      "@wishes/*": [
        "./src/app/wishes/*"
      ],
      "@widgets/*": [
        "./src/app/widgets/*"
      ]
    },
    "useDefineForClassFields": false
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "types": [
    "node",
    "tinymce"
  ],
  "exclude": [
    "./src/app/forum/*",
    "./src/environments/*",
    "./src/app/app/app.module.server.ts"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "enableI18nLegacyMessageIdFormat": false,
  }
}