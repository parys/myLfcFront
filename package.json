{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build --prod --build-optimizer",
    "build-ssr": "ng build --prod --project=angular2app",
    "build-ssr2": "ng build && ng run angular2app:server --output-path=dist/local/server",
    "build-nohash": "ng build --prod --build-optimizer --output-hashing none",
    "serve:ssr": "node dist/local/server/main.js --port 4004",
    "build:prod": "ng build --prod --configuration=production --output-path=dist/prod/browser && ng run angular2app:server:production --output-path=dist/prod/server",
    "build:test": "ng build --prod --configuration=test --output-path=dist/test/browser && ng run angular2app:server:test --output-path=dist/test/server",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "sm": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/browser/main-es2015.js dist/browser/main-es2015.js.map"
  },
  "dependencies": {
    "@angular/animations": "11.2.7",
    "@angular/cdk": "11.2.6",
    "@angular/common": "11.2.7",
    "@angular/compiler": "11.2.7",
    "@angular/compiler-cli": "11.2.7",
    "@angular/core": "11.2.7",
    "@angular/forms": "11.2.7",
    "@angular/material": "11.2.6",
    "@angular/platform-browser": "11.2.7",
    "@angular/platform-browser-dynamic": "11.2.7",
    "@angular/platform-server": "11.2.7",
    "@angular/router": "11.2.7",
    "@angular/service-worker": "11.2.7",
    "@cedx/ngx-cookies": "3.1.0",
    "@microsoft/signalr": "5.0.4",
    "@nguniversal/common": "11.2.1",
    "@nguniversal/express-engine": "11.2.1",
    "@ngxs/store": "3.7.1",
    "angular2-recaptcha": "1.1.0",
    "core-js": "3.10.0",
    "express": "4.17.1",
    "ngx-image-cropper": "2.1.2",
    "ngx-pagination": "4.1.0",
    "rxjs": "6.6.6",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.6",
    "@angular-devkit/schematics": "11.2.6",
    "@angular/cli": "11.2.6",
    "@nguniversal/builders": "11.2.1",
    "@ngxs/devtools-plugin": "3.7.1",
    "@ngxs/logger-plugin": "3.7.1",
    "@schematics/angular": "11.2.6",
    "@types/express": "4.17.11",
    "@types/node": "12.20.6",
    "@types/tinymce": "4.6.0",
    "@types/lodash": "4.14.168",
    "codelyzer": "6.0.1",
    "source-map-explorer": "2.5.2",
    "tinymce": "4.9.11",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0"
  },
  "main": "package.json",
