{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build --configuration production --build-optimizer",
    "build-ssr": "ng build --configuration production --project=angular2app",
    "build-ssr2": "ng build && ng run angular2app:server --output-path=dist/local/server",
    "build-nohash": "ng build --configuration production --build-optimizer --output-hashing none",
    "build-server-prod": "ng run angular2app:server:production --output-path=dist/prod/server",
    "serve:ssr": "node dist/local/server/main.js --port 4004",
    "build:prod": "ng build --configuration production --configuration=production --output-path=dist/prod/browser && ng run angular2app:server:production --output-path=dist/prod/server",
    "build:test": "ng build --configuration production --configuration=test --output-path=dist/test/browser && ng run angular2app:server:test --output-path=dist/test/server",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "sm": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/browser/main-es2015.js dist/browser/main-es2015.js.map"
  },
  "dependencies": {
    "@angular/animations": "12.2.11",
    "@angular/cdk": "12.2.11",
    "@angular/common": "12.2.11",
    "@angular/compiler": "12.2.11",
    "@angular/compiler-cli": "12.2.11",
    "@angular/core": "12.2.11",
    "@angular/forms": "12.2.11",
    "@angular/material": "12.2.11",
    "@angular/platform-browser": "12.2.11",
    "@angular/platform-browser-dynamic": "12.2.11",
    "@angular/platform-server": "12.2.11",
    "@angular/router": "12.2.11",
    "@angular/service-worker": "12.2.11",
    "@microsoft/signalr": "5.0.11",
    "@nguniversal/common": "12.1.2",
    "@nguniversal/express-engine": "12.1.2",
    "@ngxs/store": "3.7.2",
    "angular2-recaptcha": "1.1.0",
    "blob-polyfill": "6.0.20211015",
    "core-js": "3.18.3",
    "express": "4.17.1",
    "ngx-image-cropper": "2.1.2",
    "ngx-pagination": "4.1.0",
    "rxjs": "6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.11",
    "@angular-devkit/schematics": "12.2.11",
    "@angular/cli": "12.2.11",
    "@nguniversal/builders": "12.1.2",
    "@ngxs/devtools-plugin": "3.7.2",
    "@ngxs/logger-plugin": "3.7.2",
    "@schematics/angular": "12.2.11",
    "@types/express": "4.17.13",
    "@types/node": "14.17.27",
    "@types/tinymce": "4.6.4",
    "@types/lodash": "4.14.176",
    "codelyzer": "6.0.2",
    "source-map-explorer": "2.5.2",
    "tinymce": "4.9.11",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1"
  },
  "main": "package.json",
