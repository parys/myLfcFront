{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng build --configuration production --build-optimizer",
    "build-ssr": "ng build --configuration production --project=angular2app",
    "build-ssr2": "ng build && ng run angular2app:server --output-path=dist/local/server",
    "build-nohash": "ng build --configuration production --build-optimizer --output-hashing none",
    "build-server-prod": "ng run angular2app:server:production --output-path=dist/prod/server",
    "serve:ssr": "node dist/local/server/main.js --port 4004",
    "build:prod": "ng build --configuration production --configuration=production --output-path=dist/prod/browser && ng run angular2app:server:production --output-path=dist/prod/server",
    "build:test": "ng build --configuration production --configuration=test --output-path=dist/test/browser && ng run angular2app:server:test --output-path=dist/test/server",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "sm": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/browser/main-es2015.js dist/browser/main-es2015.js.map"
  },
  "dependencies": {
    "@angular/animations": "15.1.3",
    "@angular/cdk": "15.1.3",
    "@angular/common": "15.1.3",
    "@angular/compiler": "15.1.3",
    "@angular/compiler-cli": "15.1.3",
    "@angular/core": "15.1.3",
    "@angular/forms": "15.1.3",
    "@angular/material": "15.1.3",
    "@angular/platform-browser": "15.1.3",
    "@angular/platform-browser-dynamic": "15.1.3",
    "@angular/platform-server": "15.1.3",
    "@angular/router": "15.1.3",
    "@angular/service-worker": "15.1.3",
    "@microsoft/signalr": "7.0.2",
    "@nguniversal/common": "15.1.0",
    "@nguniversal/express-engine": "15.1.0",
    "@ngxs/store": "3.7.6",
    "angular2-recaptcha": "1.1.0",
    "blob-polyfill": "7.0.20220408",
    "core-js": "3.27.2",
    "express": "4.18.2",
    "ngx-image-cropper": "2.1.2",
    "ngx-pagination": "4.1.0",
    "rxjs": "7.8.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.4",
    "@angular-devkit/schematics": "15.1.4",
    "@angular/cli": "15.1.4",
    "@nguniversal/builders": "15.1.0",
    "@ngxs/devtools-plugin": "3.7.6",
    "@ngxs/logger-plugin": "3.7.6",
    "@schematics/angular": "15.1.4",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.11.18",
    "@types/tinymce": "4.6.5",
    "@types/lodash": "4.14.191",
    "codelyzer": "6.0.2",
    "source-map-explorer": "2.5.3",
    "tinymce": "4.9.11",
    "ts-loader": "9.4.2",
